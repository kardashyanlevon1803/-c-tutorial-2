#include <stdio.h>

int main(int argc, char *argv[]) {
    if (argc < 3) return 0;

    FILE *out = fopen(argv[argc - 1], "wb");
    if (!out) return 0;

    for (int i = 1; i < argc - 1; i++) {
        FILE *in = fopen(argv[i], "rb");
        if (!in) continue;

        int c;
        while ((c = fgetc(in)) != EOF)
            fputc(c, out);

        fclose(in);
    }

    fclose(out);
    return 0;
}
